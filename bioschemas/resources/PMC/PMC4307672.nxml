						\end{document}</tex-math><mml:math xmlns:mml="https://jats.nlm.nih.gov/ns/archiving/1.0/" id="M32"><mml:msubsup><mml:mi>H</mml:mi><mml:mn>2</mml:mn><mml:mi>i</mml:mi></mml:msubsup><mml:msup><mml:mi>T</mml:mi><mml:mi>i</mml:mi></mml:msup><mml:msubsup><mml:mi>H</mml:mi><mml:mn>1</mml:mn><mml:mrow><mml:mi>i</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msubsup></mml:math><inline-graphic xlink:href="12859_2014_440_Article_IEq7.gif"/></alternatives></inline-formula> where <inline-formula id="IEq8"><alternatives><tex-math id="M33">\documentclass[12pt]{minimal}
