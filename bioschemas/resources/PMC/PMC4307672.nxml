				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}
						$$ {A}_{ij}=1,\  if\ {d}_{ij}\le {R}_c\  and\ i\ne j $$
					\end{document}</tex-math><mml:math xmlns:mml="https://jats.nlm.nih.gov/ns/archiving/1.0/" id="M2"><mml:msub><mml:mi>A</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mspace width="0.25em"/><mml:mi mathvariant="italic">if</mml:mi><mml:mspace width="0.25em"/><mml:msub><mml:mi>d</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>â¤</mml:mo><mml:msub><mml:mi>R</mml:mi><mml:mi>c</mml:mi></mml:msub><mml:mspace width="0.25em"/><mml:mi mathvariant="italic">and</mml:mi><mml:mspace width="0.25em"/><mml:mi>i</mml:mi><mml:mo>â </mml:mo><mml:mi>j</mml:mi></mml:math><graphic xlink:href="12859_2014_440_Article_Equa.gif" position="anchor"/></alternatives></disp-formula><disp-formula id="Equb"><alternatives><tex-math id="M3">\documentclass[12pt]{minimal}
