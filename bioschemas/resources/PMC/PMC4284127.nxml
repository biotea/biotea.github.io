              <td rowspan="1" colspan="1"/>
              <td align="left" rowspan="1" colspan="1">Hypothyroidism</td>
              <td align="left" rowspan="1" colspan="1">0000821</td>
              <td align="left" rowspan="1" colspan="1">+</td>
              <td align="left" rowspan="1" colspan="1">+</td>
            </tr>
