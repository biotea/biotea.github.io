              <th align="left" rowspan="1" colspan="1">Total, n</th>
              <th align="left" rowspan="1" colspan="1">Variants</th>
              <th align="left" rowspan="1" colspan="1">n</th>
              <th align="left" rowspan="1" colspan="1">Consanguinity</th>
              <th align="left" rowspan="1" colspan="1">Mineralocorticoid Treatment</th>
            </tr>
          </thead>
