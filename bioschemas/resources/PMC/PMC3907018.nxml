                    <mml:mo>,</mml:mo>
                    <mml:mi mathvariant="normal">j</mml:mi>
                  </mml:mrow>
                </mml:mfenced>
                <mml:mo>=</mml:mo>
                <mml:mfrac>
                  <mml:mrow>
                    <mml:mi mathvariant="normal">g</mml:mi>
                    <mml:mfenced open="(" close=")">
                      <mml:mrow>
                        <mml:mi mathvariant="normal">i</mml:mi>
                        <mml:mo>,</mml:mo>
                        <mml:mi mathvariant="normal">j</mml:mi>
                      </mml:mrow>
                    </mml:mfenced>
                  </mml:mrow>
                  <mml:mrow>
                    <mml:mi>sd</mml:mi>
                    <mml:mfenced open="(" close=")">
                      <mml:mrow>
                        <mml:mi mathvariant="normal">g</mml:mi>
                        <mml:mfenced open="(" close=")">
                          <mml:mi mathvariant="normal">i</mml:mi>
                        </mml:mfenced>
                      </mml:mrow>
                    </mml:mfenced>
                    <mml:mo>+</mml:mo>
                    <mml:mi>s</mml:mi>
                    <mml:msub>
                      <mml:mi>d</mml:mi>
                      <mml:mn>10</mml:mn>
                    </mml:msub>
                    <mml:mfenced open="(" close=")">
                      <mml:mi mathvariant="normal">g</mml:mi>
                    </mml:mfenced>
                  </mml:mrow>
